<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= link_to image_tag(pin.image.url(:medium)), pin %>

      <div class="panel-body center">
        
        <p><em><%= pin.description %></em></p><br/>
        <%= link_to 'Details', pin_path(pin), class: 'btn btn-default' %>
        <%= link_to 'Compare', "#", class: 'btn btn-default' %>
      </div>
        
     
    </div>
  <% end %>
</div>
